    "constructs": "^10.0.0",
    "dotenv": "^16.0.3",
    "envalid": "^7.3.1",
    "pg": "^8.9.0",
    "pg-native": "^3.0.1",
    "source-map-support": "^0.5.21",
    "superstruct": "^1.0.3"
  }
