  "bin": {
    "resports-aws-cdk": "bin/resports-aws-cdk.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "cdk": "cdk",
    "prepare": "husky install",
    "test:ci": "jest --ci --passWithNoTests",
    "prettier": "prettier --check .",
    "lint": "npx eslint src/* lib/*"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --fix",
      "prettier --write"
    ],
