  "devDependencies": {
    "@databases/pg-test": "^3.1.2",
    "@types/aws-lambda": "^8.10.110",
    "@types/jest": "^29.4.0",
    "@types/node": "18.11.18",
    "@types/pg": "^8.6.6",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "aws-cdk": "2.64.0",
    "eslint": "^8.34.0",
    "husky": "^8.0.3",
    "jest": "^29.4.1",
    "lint-staged": "^13.1.2",
    "prettier": "^2.8.4",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^3.14.2",
    "typescript": "~4.9.5"
  },
  "dependencies": {
    "@aws-cdk/aws-apigatewayv2-alpha": "^2.64.0-alpha.0",
